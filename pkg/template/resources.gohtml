         <script type="text/javascript">const navbarRightAll=document.querySelectorAll(".navbar-right"),navbarBurgerAll=document.querySelectorAll(".navbar-burger");Array.from(navbarBurgerAll).forEach((e,t)=>{e.addEventListener("click",function(){Array.from(navbarRightAll).forEach((e,n)=>{t!==n&&navbarRightAll[n].classList.contains("is-visible")&&(navbarRightAll[n].classList.remove("is-visible"),navbarBurgerAll[n].classList.remove("is-active"))});const e=document.querySelector(".modal-backdrop");e&&e.classList.contains("is-show")&&e.classList.remove("is-show");const n=document.querySelector(".nav-item.is-active");n&&n.classList.contains("is-active")&&n.classList.remove("is-active"),navbarRightAll[t].classList.toggle("is-visible")})})</script>
