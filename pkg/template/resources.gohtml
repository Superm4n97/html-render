<div class="ac-system-body is-fullwidth">
   <form action="/bind" method="post" class="ac-system-content kube-bind-form">
      <div class="inner-header" style="top: 50px;">
         <div class="container">
            <div class="is-flex is-justify-content-space-between">
            <div class="header-items is-flex is-align-items-center">
               <div class="header-item">
                  <h4 class="">All Bind</h4>
               </div>
            </div>
            <div class="header-items is-flex is-align-items-center">
               <input type="submit" value="BIND DATABASES" class="button ac-button is-primary"/>
            </div>
         </div>
         </div>
      </div>

      {{ if le (len .CRDs) 10 }}
      <div class="container">
         <div class="kube-bind-inner mb-32 mt-16">
            <input type="hidden" id="sessionID" name="sessionID" value="{{.SessionID}}">
            {{$sid := .SessionID}}
            <div class="columns is-multiline">
               {{ range $idx, $crd := .CRDs}}
                  {{ $gr := list $crd.Resource $crd.Group | compact | join "." }}

                     <div class="column is-3-desktop is-2-tablet is-1-mobile">
                     <input type="checkbox" class="is-hidden" id="crd_{{$idx}}" name="crd" value="{{$gr}}" {{if $crd.Bound}} disabled{{end}}>
                     <label for="crd_{{$idx}}" class="single-card">
                        <div class="icon">
                              <img src="{{$crd.Icon}}"/>
                        </div>
                        <h4 class="is-size-5 has-text-weight-semibold is-capitalized">{{$crd.Kind}}</h4>
                        <ul class="is-flex is-flex-direction-column">
                              <li class="list-group-item"><span class="has-text-black-bis">Group:</span> <span>{{$crd.Group}}</span></li>
                              <li class="list-group-item"><span class="has-text-black-bis">Scoped:</span> <span>{{$crd.Scoped}}</span></li>
                        </ul>
                     </label>
                     </div>

               {{end}}
            </div>
         </div>
      </div>
      {{ else }}
      <div class="container">
         <!-- list items start-->
         <div class="ac-table-container table-container b-1 mt-16">
            <table class="table ac-table is-bordered is-fullwidth">
               <thead>
                  <tr class="is-hoverless" data-testid="ac-table-row">
                     <th class="width-60">
                     <!-- <input  id="checkbox-primary1" type="checkbox" name="checkbox-primary"> -->
                     </th>
                     <th>
                        <span>TITLE</span>
                     </th>
                     <th>
                        <span>GROUP</span>
                     </th>
                     <th>
                        <span>SCOPED</span>
                     </th>
                  </tr>

               </thead>
               <tbody>
               {{ range $idx, $crd := .CRDs}}
               {{ $gr := list $crd.Resource $crd.Group | compact | join "." }}
                  <tr>
                     <td class="{{if $crd.Bound}}is-disabled{{end}}">
                        <input type="checkbox"  id="crd_{{$idx}}" name="crd" {{if $crd.Bound}} disabled{{end}} value="{{$gr}}"/>
                     </td>
                     <td class="{{if $crd.Bound}}is-disabled{{end}}">
                        <label class="is-flex is-align-items-center gap-8" for="crd_{{$idx}}">
                           <span class="icon">
                              <img src="{{$crd.Icon}}"/>
                           </span>
                           <h6>{{$crd.Kind}}</h6>
                        </label>
                     </td>
                     <td class="{{if $crd.Bound}}is-disabled{{end}}">{{$crd.Group}}</td>
                     <td class="{{if $crd.Bound}}is-disabled{{end}}">{{$crd.Scoped}}</td>
                  </tr>
               {{ end }}
               </tbody>
            </table>
         </div>
         <!-- list items end-->
      </div>
      {{ end }}

   </form>
</div>



