    <style>
        .container {
            padding: 0 16px;
        }
        .form-wrapper {
            .icon {
                width: 100%;
                display: flex;
                align-items: center;
                margin-bottom: 8px;
                height: auto;
                img {
                    width: 52px;
                    height: auto;
                }
            }
            max-width: 920px;
            margin: 60px auto;
            .form-inner {
                display: grid;
                grid-template-columns: repeat(auto-fill, minmax(min(250px, 100%), 1fr));
                align-content: center;
                justify-content: center;
                gap: 16px;
                margin-bottom: 32px;
            }
        }

        .form-inner input[type="checkbox"]:checked + label.card{
            border: 2px solid #4D4DCB;
        }

        .form-inner input[type="checkbox"]:disabled + label.card{
            cursor: not-allowed;
            opacity: 0.6;
        }
        .card {
            cursor: pointer;
            border: 2px solid transparent;
            transition: 0.3s ease-in-out;
        }
        .card:hover {
            border: 2px solid #e7e7e7;
        }
        ul {
            margin-bottom: 8px;
            gap: 4px;
            li{
                line-height: 120%;
                text-align: center;
                span {
                    line-break: anywhere;
                    font-size: 15px;
                }
            }
        }
        .button.is-primary {
            background-color: #4D4DCB;
            padding: 10px 32px;
            line-height: 1;
            &:hover {
                background-color: #4444bc;
            }
        }
       
    </style>
</head>
